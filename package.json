    "axios": "^1.10.0",
    "capacitor-email-composer": "^5.0.0",
    "crypto-js": "^4.2.0",
    "html2canvas": "^1.4.1",
    "ionicons": "^7.0.0",
    "jspdf": "^3.0.1",
    "pm2": "^6.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",

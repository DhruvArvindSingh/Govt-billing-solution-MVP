{
  "name": "GovtInvoiceNew",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --host 0.0.0.0 --port 3000",
    "analyze": "npx vite-bundle-analyzer dist",
    "type-check": "tsc --noEmit",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "test": "npm run type-check && npm run test.unit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "generate-pwa-assets": "pwa-assets-generator",
    "clean": "rm -rf dist node_modules/.vite",
    "build:android": "npm run build:prod && npx cap sync android && npx cap build android",
    "android": "npx cap open android"
  },
  "dependencies": {
    "@capacitor/android": "5.7.0",
